{
  "name": "covid19",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isReady": "yarn format && yarn lint && yarn build",
    "build:serve": "yarn build && serve -s build",
    "build:profile": "yarn build --profile && serve -s build",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "analyzer": "yarn build && npx cra-bundle-analyzer"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@hookform/resolvers": "^2.8.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/dompurify": "^2.3.1",
    "@types/jest": "^27.0.2",
    "@types/react-alert": "^5.2.1",
    "@types/react-slick": "^0.23.5",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@typescript-eslint/scope-manager": "^4.32.0",
    "dompurify": "^2.3.3",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "firebase": "^9.1.1",
    "next": "11.1.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "query-string": "^7.0.1",
    "rc-pagination": "^3.1.9",
    "react": "17.0.2",
    "react-alert": "^7.0.3",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.1",
    "react-redux": "^7.2.5",
    "react-slick": "^0.28.1",
    "redux-logger": "^3.0.6",
    "sass": "^1.42.1",
    "serve": "^12.0.1",
    "ts-jest": "^27.0.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/react": "17.0.27",
    "autoprefixer": "^10.3.6",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-risxss": "^2.1.0",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "precss": "^4.0.0",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  }
}
